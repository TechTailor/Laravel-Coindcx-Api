{
    "name": "techtailor/laravel-coindcx-api",
    "description": "A Laravel Wrapper for the CoinDCX API.",
    "keywords": [
        "coindcx",
        "coindcx-api",
        "laravel-coindcx",
        "laravel-coindcx-api"
    ],
    "homepage": "https://github.com/TechTailor/Laravel-CoinDCX-Api",
    "license": "MIT",
    "authors": [
        {
            "name": "Moinuddin S. Khaja",
            "email": "hello@moinuddin.info",
            "homepage": "https://moinuddin.info",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/contracts": "^7.0|^8.0",
        "illuminate/http": "^7.0|^8.0",
        "illuminate/support": "^7.0|^8.0",
        "spatie/laravel-package-tools": "^1.9"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "TechTailor\\CoinDCXApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TechTailor\\CoinDCXApi\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "TechTailor\\CoinDCXApi\\CoinDCXApiServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
